              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"teamId\": \"{{teamId}}\",\n  \"channelId\": \"{{channelId}}\",\n  \"content\": \"<p>Hello from API!</p>\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/messaging/channel/announcement",
              "host": ["{{baseUrl}}"],
              "path": ["api", "messaging", "channel", "announcement"]
